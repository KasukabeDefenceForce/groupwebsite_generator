<html>
<head>
    <title> {{ general.website_title }} </title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="assets/css/main.css" />
    <link rel="stylesheet" href="assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets/css/style.css">
</head>
<body class="right-sidebar is-preload">
    <div id="page-wrapper">
        <!-- Header -->
        <div id="header-wrapper">
            <div id="header" class="container">
                <!-- Nav -->
                <nav id="nav">
                    <ul>
                        <li>
                            {% for page in general["navigation_bar"] %}
                        <li>
                            <a href="{{ page_link(page) }}.html">{{ page }}</a>
                            {% if page in general %}
                            <ul>
                                {% for sub_page in general[page] %}
                                <li><a href="sub_research/{{ page_link(sub_page) }}.html">{{sub_page}}</a></li>
                                {% endfor %}
                            </ul>
                            {% endif %}
                        </li>
                        {% endfor %}
                        </li>
                    </ul>
                </nav>
            </div>
        </div>



        <!-- Features 1 -->
        <div class="wrapper">
            <div class="container" id="main">
                <div class="row">
                    <div class="col-md-6">
                        <header>
                            <h2> {{ contact.form_title }} </h2>
                        </header>
                         <form method="post" action="#">
                            <div class="row gtr-50">
                                <div class="col-6 col-12-mobile">
                                    <input name="name" placeholder="{{ contact.field1 }}" type="text" />
                                </div>
                                <div class="col-6 col-12-mobile">
                                    <input name="email" placeholder="{{ contact.field2 }}" type="text" />
                                </div>
                                <div class="col-12">
                                    <textarea name="message" placeholder="{{ contact.field3 }}"></textarea>
                                </div>
                                <div class="col-12">
                                    <ul class="actions">
                                        <li><input type="submit" value="{{ contact.button1 }}" /></li>
                                        <li><input type="reset" value="{{ contact.button2 }}" /></li>
                                    </ul>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-md-6">
                        <div class="map-wrapper">
                            <div style="width: 100%; "><iframe width="100%" height="100%" frameborder="0" scrolling="no"
                                    marginheight="0" marginwidth="0"
                                    src="https://maps.google.com/maps?width=100%25&amp;height=600&amp;hl=en&amp;q=567%20Wilson%20Rd,%20East%20Lansing,%20MI%2048824,%20United%20States+(MSU%20Group)&amp;t=&amp;z=15&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"><a
                                        href="https://www.maps.ie/distance-area-calculator.html">area maps</a></iframe>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
            <!-- footer start -->
            <footer class="footer">
                <div class="footer_top">
                    <div class="container">
                        <div class="row">
                            <div class="col-xl-4 col-md-6 col-lg-4">
                                <div class="footer_widget">
                                    <div class="logo_footer">
                                        <a href="index.html">
                                            <img src="img/logo.png" alt="">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-md-6 col-lg-3">
                                <div class="footer_widget">
                                    <h3 class="footer_title">
                                        Address
                                    </h3>
                                    <ul style="padding-right: 40%;">
                                        <li>
                                            <h6 class="icon maps fa-map"> {{ general.address }}</h6>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-xl-3 col-md-6 col-lg-3">
                                <div class="footer_widget">
                                    <h3 class="footer_title">
                                        Useful Links
                                    </h3>
                                    <ul>
                                        {% for link_text, link_url in general.useful_link.items() %}
                                        <li><a href="{{ link_url }}" target="_blank">{{ link_text }}</a></li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            </div>
                            <div class="col-xl-2 col-md-6 col-lg-2">
                                <div class="footer_widget">
                                    <h3 class="footer_title">
                                        Social Links
                                    </h3>
                                    <ul>
                                        {% for platform, link in general.social_media_links.items() %}
                                        <li><a href="{{ link }}">{{ platform }}</a></li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="copy-right_text">
                    <div class="container">
                        <div class="row">
                            <div class="col-xl-12">
                                <p class="copy_right text-center">
                                    {{ general.footer_message }}
                                    <script>document.write(new Date().getFullYear());</script>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
            <!--footer end-->
            <!-- Scripts -->
            <script src="assets/js/jquery.min.js"></script>
            <script src="assets/js/jquery.dropotron.min.js"></script>
            <script src="assets/js/browser.min.js"></script>
            <script src="assets/js/breakpoints.min.js"></script>
            <script src="assets/js/util.js"></script>
            <script src="assets/js/main.js"></script>
    
    </div>
</body>
</html>