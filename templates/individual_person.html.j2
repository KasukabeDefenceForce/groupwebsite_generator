    <section style="padding-left: 10%; padding-right:10%;">
        {% for data in [ "education", "experience", "projects", "awards", "outreach" ]
        %}
        {% if data in person %}

        {% for key, value in person[data].items() %}
        {% if key == "section" and value %}
        <div style="padding-top:5%;" class="title-heading-w3 mx-auto text-center mb-sm-5 mb-4 pb-xl-4">
            <h3 class="w3l-title-main">{{ value }}</h3>
