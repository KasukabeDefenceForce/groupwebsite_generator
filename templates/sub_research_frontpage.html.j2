    <!-- Blog Section -->
    <section class="w3l-blog">
        <div class="blog py-5" id="Newsblog">
            {% for project_list in [astrophysical_transients, computational_metascience] %}
            {% if project_list|length > 0 %}
            <div class="text-center" style="padding-top: 5%;">
                <h1> {{ project_list[0]['category'] }}</h1>
                <br>
                <div class="container py-lg-5 py-md-4 py-2">
                    <div class="row">
                        <p style="padding-bottom: 3%; text-align:left; font-size: 20px; color:black;">{{ project_list[0]['about'] }}</p>
                    </div>
                </div>
            </div>
            <div class="container py-lg-5 py-md-4 py-2">
                <div class="row">
                    {% for project in project_list %}
                    {% if not loop.first %}
                    <div class="col-lg-4 col-md-6 item">
                        <div class="card">
                            <div class="card-header p-0 position-relative">
                                <a href="{{ project_list[0]['category']|replace(' ', '_')|lower }}/{{ page_link(project.name) }}.html"
                                    class="zoom d-block">
                                    <img class="card-img-bottom d-block" src="../{{ project.image_path['cover_image'] }}"
                                        alt="array">
                                </a>
                                <div class="date">
                                    <p>{{ project.article_date }} </p>
                                </div>
                            </div>
                            <div class="card-body blog-details">
                                <a href="{{ project_list[0]['category']|replace(' ', '_')|lower }}/{{ page_link(project.name) }}.html"
                                    class="blog-desc"> {{ project.name }} </a>
                                <div class="price-review mb-1 align-items-center">
                                    <p>{{ project.short_description }} </p>
                                </div>

                            </div>
                            <div class="card-footer">
                                <div class="author align-items-center">
                                    <a href="#author" class="post-author">
                                        <img src="../members/{{ project.authon_image }}" alt=""
                                            class="img-fluid rounded-circle">
                                    </a>
                                    <ul class="blog-meta">
                                        <li>
                                            <span class="meta-value">Article by </span><a href="#author">{{
                                                project.author }}</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    {% endif %}
                    {% endfor %}
                </div>
            </div>
            {% endif %}
            {% endfor %}
        </div>
    </section>
